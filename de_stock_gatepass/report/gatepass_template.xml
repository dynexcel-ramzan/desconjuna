<?xml version="1.0" encoding="utf-8"?>

<odoo>

	<report id="gate_pass_report_id"
		string="GatePass Report"
		model="stock.gatepass"
		report_type="qweb-pdf"
		name="de_stock_gatepass.gate_pass_report"
		file="de_stock_gatepass.gate_pass_report"
		print_report_name="'GatePass - %s' % (object.name)"
	/>
	<record id="paperformat_stock_gatepass_portrait" model="report.paperformat">
		<field name="name">PF Gatepass Report</field>
		<field name="default" eval="True"/>
		<field name="format">A4</field>
		<field name="page_height">0</field>
		<field name="page_width">0</field>
		<field name="orientation">Portrait</field>
		<field name="margin_top">10</field>
		<field name="margin_bottom">10</field>
		<field name="margin_left">7</field>
		<field name="margin_right">7</field>
		<field name="header_line" eval="False"/>
		<field name="header_spacing">3</field>
		<field name="dpi">90</field>
	</record>

</odoo>