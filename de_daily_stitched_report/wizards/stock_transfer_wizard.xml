<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_stock_transfer_wizard" model="ir.ui.view">
            <field name="name">stock.transfer.wizard</field>
            <field name="model">stock.transfer.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="from_date"/>
                        </group>
                        <group>
                            <field name="to_date"/>
                        </group>
                    </group>

                    <footer>
                    <button name="print_production_pdf"
                            string="Production PDF"
                            type="object"
                            class="oe_highlight"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>
