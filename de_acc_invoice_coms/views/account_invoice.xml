<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!--Add commission in invoice list view-->
        <record id="inherit_account_move_tree_view" model="ir.ui.view">
            <field name="name">inherit.account.move.tree.view</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_out_invoice_tree"/>
            <field name="arch" type="xml">
                <field name="amount_residual_signed" position="after">
                    <field name="amount_commission" groups="de_sale_comission.group_comission_rate"/>
                </field>

            </field>
        </record>
    </data>
</odoo>