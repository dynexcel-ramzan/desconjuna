    <record id="paperformat_mo_wise_so_portrait" model="report.paperformat">
        <field name="name">PF MO wise SO Portrait</field>
        <field name="default" eval="True"/>
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">10</field>
        <field name="margin_bottom">15</field>
        <field name="margin_left">7</field>
        <field name="margin_right">7</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">3</field>
        <field name="dpi">90</field>
    </record>

    <record id="action_mo_wise_so_report" model="ir.actions.report">
        <field name="name">MO Wise SO Report</field>
        <field name="model">sale.order</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">de_mo_wise_so_report.report_mo_wise_so_document</field>
    </record>
